{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es2017",
        "declaration": false,
        "module": "commonjs",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
    }
}
